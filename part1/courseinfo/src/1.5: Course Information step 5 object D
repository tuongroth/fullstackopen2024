import React from 'react';
import ReactDOM from 'react-dom';


const App = () => {
  const course = {
    name: 'Half Stack application development',
    parts: [
      {
        name: 'Fundamentals of React',
        exercises: 10
      },
      {
        name: 'Using props to pass data',
        exercises: 7
      },
      {
        name: 'State of a component',
        exercises: 14
      }
    ]
  };


  // Update the name property
  course.name = 'Fundamentals of React';


  // Add a new property using dot notation
  course.teacher = 'John Doe';


  // Add a new property using bracket notation
  course['language'] = 'JavaScript';


  console.log(course); // Output the updated course object


  return (
    <div>
     <Header parts={course} />
          <Content parts={parts} />
          <Total parts={parts} />
    </div>
  );
};

export default App;

  
